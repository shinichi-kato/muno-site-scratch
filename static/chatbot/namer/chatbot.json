{
    "avatar": "plant-fairy.svg",
    "name": "ミドリ",
    "backgroundColor": "#c7a2e0",
    "encoder": "PatternEncoder",
    "stateMachine": "NamingStateMachine",
    "decoder": "HarvestDecoder",
    "precision": 0.3,
    "script": [
        {
            "intent": "start",
            "in": [
                "こんにちは"
            ],
            "out": [
                "こんにちは"
            ]
        },
        {
            "intent": "absent",
            "in": ["{NOP}"],
            "out":["{NOP}"]
        },
        {
            "intent": "stand_by",
            "in": [
                "{NOP}"
            ],
            "out": [
                "{NOP}"
            ]
        },
        {
            "intent": "summon",
            "in": [
                "{BOT_NAME}"
            ],
            "out": [
                "はーい"
            ]
        },
        {
            "intent": "naming",
            "in": [
                "(.+?)って呼んでいい？"
            ],
            "out": [
                "{LAST}でいいですか？"
            ]
        },
        {
            "intent": "renaming",
            "in": [
                "(.+?)だ",
                "(.+?)って呼んでいい？"
            ],
            "out": [
                "じゃあ {LAST} ？",
                "{LAST}でいいですか？"
            ]
        },
        {
            "intent": "confirm",
            "in": [
                "^うん。?$",
                "^そう",
                "^やったー*"
            ],
            "out": [
                "分かりました(笑",
                "{LAST}です。よろしくね"
            ]
        },
        {
            "intent": "break",
            "in": [
                "やっぱやめ"
            ],
            "out": [
                "？"
            ]
        },
        {
            "intent": "not_found",
            "in": [
                "{nop}"
            ],
            "out": [
                "そうなの？","よくわからないよ・・・","？"
            ]
        }
    ]
}